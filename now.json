{
    "name": "adele",
    "version": 2,
    "builds": [
      {"src": "build.sh", "use": "@now/static-build"}
    ],
    "routes": [
      { "handle": "filesystem"},
      { "src": "/(.*)", "dest": "index.html"}
      { "src": "/src/data/(.*)", "headers": {"cache-control": "s-maxage=0"}, "dest": "/index.html" },
    ]
}
