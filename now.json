{
    "name": "adele",
    "version": 2,
    "builds": [
      {"src": "build.sh", "use": "@now/static-build"}
    ],
    "routes": [
      { "handle": "filesystem"},
      { "src": "/(.*)", "dest": "index.html"}
    ],
    "headers": [
      {
        "source": "/src/data/data.JSON",
        "headers" : [
          {
            "key" : "Cache-Control",
            "value" : "public, max-age=0, must-revalidate"
          }
        ]
      }
    ]
}
